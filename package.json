{
  "name": "project-e",
  "version": "1.0.0",
  "description": "Project-E",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:cover": "jest --coverage",
    "start": "ts-node-dev --poll src/index.ts",
    "before-build": "npm run clean && yarn && npm run lint && npm run type-check",
    "build:ts": "npm run before-build && tsc -p .",
    "model:generate": "npx sequelize-cli model:generate --name",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:undo": "npx sequelize-cli db:migrate:undo",
    "db:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "db:migrate:fresh": "npx sequelize-cli db:migrate:undo:all && npm run db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:seed:create": "npx sequelize-cli seed:generate --name",
    "db:drop": "npx sequelize-cli db:drop",
    "db:reset": "npm run db:drop && npx sequelize-cli db:create && npm run db:migrate && npm run db:seed"
  },
